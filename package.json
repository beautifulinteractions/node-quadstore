{
  "name": "quadstore",
  "version": "6.0.1",
  "description": "A LevelDB-backed graph database for Node.js and the browser. Supports quads, RDF/JS interfaces and SPARQL queries.",
  "keywords": [
    "node",
    "nodejs",
    "quadstore",
    "graph",
    "rdf",
    "database",
    "sparql",
    "rdfjs",
    "browser"
  ],
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "test-rdf": "rdf-test-suite -c .rdf-test-suite-cache/ test-rdf/engine.js https://w3c.github.io/rdf-tests/sparql11/data-sparql11/manifest-all.ttl -s http://www.w3.org/TR/sparql11-query/ -t http://www.w3.org/2009/sparql/docs/tests/data-sparql11/aggregates/",
    "ts:build": "tsc --project .",
    "ts:watch": "tsc --watch --project ."
  },
  "files": [
    "index.js",
    "index-browser.js",
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/beautifulinteractions/node-quadstore.git"
  },
  "devDependencies": {
    "@rdfjs/data-model": "^1.1.2",
    "@types/abstract-leveldown": "^5.0.1",
    "@types/asynciterator": "^1.1.4",
    "@types/encoding-down": "^5.0.0",
    "@types/levelup": "^4.3.0",
    "abstract-leveldown": "^6.3.0",
    "benchmark": "^2.1.4",
    "fs-extra": "^8.1.0",
    "husky": "^3.1.0",
    "leveldown": "^5.4.1",
    "memdown": "^5.1.0",
    "mocha": "^7.0.0",
    "n3": "^1.3.5",
    "nyc": "^15.0.0",
    "rdf-test-suite": "^1.13.1",
    "rdflib": "^1.0.7-2",
    "should": "^13.2.3",
    "typescript": "^3.9.3",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "asynciterator": "^2.0.1",
    "encoding-down": "^6.3.0",
    "js-sorted-set": "^0.2.0",
    "levelup": "^4.3.2",
    "lodash.flatmap": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "p-iteration": "^1.1.8",
    "sparqljs": "^3.0.1"
  },
  "author": "Jacopo Scazzosi <me@jacoscaz.com>",
  "contributors": [
    "Matteo Murgida <teomurgi@gmail.com>"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/beautifulinteractions/node-quadstore",
  "bugs": "https://github.com/beautifulinteractions/node-quadstore/issues",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
}
